@model LogReportProject.BO.LogJsonResponse

@{
    ViewData["Title"] = "GetLogReport";
}

<style>
    table {
        font-family: arial, sans-serif;
        border-collapse: collapse;
        width: 100%;
    }

    td, th {
        border: 1px solid #dddddd;
        text-align: left;
        padding: 8px;
    }

    tr:nth-child(even) {
        background-color: #dddddd;
    }
</style>
<div>
    <h4>LogJsonResponse</h4>
    <hr />

    <table class="table table-striped border">
        <tr class="table-secondary">
            <th>
                User ID
            </th>


            <th>
                Time Stamp With Pages Visited by Individual Users
            </th>
        </tr>


    @if (Model.MappedPage.Count() > 0)
    {
        <h1> Total Number of Users @Model.MappedPage.Count()</h1>
       
            @foreach (var item in Model.MappedPage)
            {
                <tr>

                    <td> @item.Key</td>
                    <td>
                        @foreach (var listval in item.Value)
                        {
                            @*<td>=></td>*@
                            <table>
                                <tr>
                                    <td>@listval </td>
                                </tr>
                            </table>
                        }
                    </td>
                </tr>
            }
      
    }

    </table>


